{
    "name": "willydamtchou/symfony-utilities-bundle",
    "description": "Symfony Utilities Bundle",
    "type": "symfony-bundle",
    "version": "1.0.0",
    "license": "proprietary",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Willy DAMTCHOU",
            "email": "willydamtchou@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/runtime": "6.4.*"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.8",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/phpunit-bridge": "^6.3",
        "vimeo/psalm": "^5.14"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Willydamtchou\\SymfonyUtilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Willydamtchou\\SymfonyUtilities\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
